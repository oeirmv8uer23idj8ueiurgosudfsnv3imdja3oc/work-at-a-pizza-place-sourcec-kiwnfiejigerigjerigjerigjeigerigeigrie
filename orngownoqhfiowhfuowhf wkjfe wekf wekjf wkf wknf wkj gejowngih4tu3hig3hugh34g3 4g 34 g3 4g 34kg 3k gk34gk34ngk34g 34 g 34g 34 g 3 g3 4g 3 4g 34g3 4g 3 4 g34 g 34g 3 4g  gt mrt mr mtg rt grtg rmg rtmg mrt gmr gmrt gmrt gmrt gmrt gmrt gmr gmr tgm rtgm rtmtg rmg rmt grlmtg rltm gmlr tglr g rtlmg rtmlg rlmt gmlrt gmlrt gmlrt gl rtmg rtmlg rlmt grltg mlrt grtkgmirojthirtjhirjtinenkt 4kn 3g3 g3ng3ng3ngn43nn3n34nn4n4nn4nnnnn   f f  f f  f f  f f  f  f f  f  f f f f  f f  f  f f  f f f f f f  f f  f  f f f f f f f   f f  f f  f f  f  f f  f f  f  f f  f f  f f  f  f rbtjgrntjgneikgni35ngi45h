	Callback = function(awdadsasd)
		trollellemethoyud = awdadsasd
	end    
})

trol:AddButton({
	Name = "Start Troll Function",
	Callback = function()
		if trollellemethoyud == "None" then return end
		if weokfowefo == nil or weokfowefo == "" then return end
		safeskripz()
		if trollellemethoyud == "Fling" then
			flingplr = true
			oldcf = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			if game.Players.LocalPlayer.Character.Humanoid.Sit == true then task.wait()
				game.Players.LocalPlayer.Character.Humanoid = false
			end
			for m=1,2 do task.wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,0,0)
			end
			task.wait(0.2)
			for wef,asd in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				if asd:IsA("Part") or asd:IsA("BasePart") then
					asd.Anchored = true
				end
			end
			game.Players.LocalPlayer.Character.HipHeight = 450
			repeat task.wait()
    	        local pos = {x=0, y=0, z=0}
        		pos.x = trolselected.Character.HumanoidRootPart.Position.X
        		pos.y = trolselected.Character.HumanoidRootPart.Position.Y
        		pos.z = trolselected.Character.HumanoidRootPart.Position.Z
        		pos.x = pos.x + trolselected.Character.HumanoidRootPart.Velocity.X / 2
        		pos.y = pos.y + trolselected.Character.HumanoidRootPart.Velocity.Y / 2
        		pos.z = pos.z + trolselected.Character.HumanoidRootPart.Velocity.Z / 2
    		    lp.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(pos.x,pos.y,pos.z))
			until trolselected.Character.HumanoidRootPart.Velocity.magnitude <= 750
			game.Players.LocalPlayer.Character.Head:Destroy()
			repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("Head")
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldcf
			flingplr = false
		end
  	end    
})

